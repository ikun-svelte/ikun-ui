{
	"name": "@ikun-ui/checkbox",
	"version": "0.0.13-beta.3",
	"type": "module",
	"main": "src/index.ts",
	"types": "src/index.d.ts",
	"svelte": "src/index.ts",
	"keywords": [
		"svelte",
		"svelte3",
		"web component",
		"component",
		"react",
		"vue",
		"svelte-kit",
		"dx"
	],
	"files": [
		"dist",
		"package.json"
	],
	"scripts": {
		"build": "npm run build:js && npm run build:svelte",
		"build:js": "tsc -p . --outDir dist/ --rootDir src/",
		"build:svelte": "svelte-strip strip src/ dist",
		"publish:pre": "node ../../scripts/pre-publish.js",
		"publish:npm": "pnpm run publish:pre && pnpm publish --no-git-checks --access public"
	},
	"publishConfig": {
		"access": "public",
		"main": "dist/index.js",
		"module": "dist/index.js",
		"svelte": "dist/index.js",
		"types": "dist/index.d.ts"
	},
	"dependencies": {
		"@ikun-ui/checkbox-group": "workspace:*",
		"@ikun-ui/icon": "workspace:*",
		"@ikun-ui/form": "workspace:*",
		"@ikun-ui/utils": "workspace:*",
		"baiwusanyu-utils": "^1.0.16",
		"clsx": "^2.0.0"
	},
	"devDependencies": {
		"@tsconfig/svelte": "^5.0.2",
		"svelte-strip": "^3.0.1",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2"
	}
}
