{
	"name": "@ikun-ui/utils",
	"version": "0.0.13-beta.2",
	"sideEffects": false,
	"files": [
		"dist"
	],
	"main": "./src/index.ts",
	"scripts": {
		"build": "tsup --config tsup.config.ts",
